<%- include ../_header %>

<h1 class="display-4"><%=title%></h1>

<div class="col-sm-12">    
    <div class="card card-block">
        <% for (var i=0; i<comment.length; i++) { %>
            <p class="card-text"><%-comment[i]%>
        <% } %>
    </div> 
    
    <div class="card card-block">
        <h4 class="card-title"><%-prototype.title%></h4>
        <% for (var i=0; i<prototype.comment.length; i++) { %>
            <p class="card-text"><%-prototype.comment[i]%>
        <% } %>
    </div> 
</div>

<%- include ../_footer %>





